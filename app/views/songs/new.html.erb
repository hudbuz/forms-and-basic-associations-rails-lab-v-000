<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%=f.label :artist_name %>
  <%= f.text_field :artist_name, list: "artist_name" %>
  <datalist id="artist_name">
  <%= Artist.all.each do |artist| %>
  <option value="<%= artist.name %>">
  <% end %></datalist><br>

  <%=f.label :genre_id %>
  <%= f.collection_check_boxes :genre_id, Genre.all, :id, :name %><br>

  

  <%= f.submit %>
<% end %>


